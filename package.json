{
    "name": "editorzoom",
    "displayName": "Editor Zoom",
    "description": "Zooms the editor font instead of the entire workspace",
    "version": "1.0.0",
    "publisher": "zolocode",
    "homepage": "zolo.io",
    "license": "MIT",
    "repository": 
    { 
        "type" : "git",
        "url" : "https://github.com/zolo/editorzoom.git"
    }
    ,
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "zoom", "editor", "font"
    ],
    "activationEvents": [
        "onCommand:extension.editorZoomIn",
        "onCommand:extension.editorZoomOut"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.editorZoomIn",
                "key": "ctrl+9",
                "mac": "cmd+9"
            },
            {
                "command": "extension.editorZoomOut",
                "key": "ctrl+8",
                "mac": "cmd+8"
            }
        ],
        "commands": [
            {
                "command": "extension.editorZoomIn",
                "title": "Zoom in editor"
            }, 
            {
                "command": "extension.editorZoomOut",
                "title": "Zoom out editor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}